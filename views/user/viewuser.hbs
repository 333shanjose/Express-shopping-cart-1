 <section>
          <div class="container mt-5">
        <div class="row">
          {{#each products}}
          <div class="col-md-3 ">
        

        <div class="card,mt-5px" style="width: 15rem;height:25rem";>
  <img src="/productimages/{{this._id}}.jpg" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <p>Rs{{this.Price}}</p>
    <p class="card-text">{{this.discription}}</p>
    
    <button onclick="addToCart('{{this._id}}')" class="btn btn-primary">Add to cart</button>
  </div>
  
</div>

</div>


{{/each}}
</div>
</div>

    </section>